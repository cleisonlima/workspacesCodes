package Scanner; // Certifique-se de que o pacote é o mesmo ao longo do arquivo

public class Scanner{ // Mudei o nome da classe para ScannerStudo

    public static void main(String[] args) {

        Scanner sc = new Scanner(System.in); // Crie o objeto Scanner para entrada do usuário

        String x;
        x = sc.next();
        System.out.println("Você digitou: " + x);

        sc.close();
    }
}

